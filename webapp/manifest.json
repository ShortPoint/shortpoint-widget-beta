{
	"_version": "1.3.0",
	"sap.app": {
		"_version": "1.1.0",
		"id": "hcp://shortpointwidgetbeta/manifest.json",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "4.7.2"
		},
		"title": "ShortPoint Widget",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.32.0"
		}
	},
	"sap.ui": {
		"_version": "1.1.0",
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_bluecrystal"
		]
	},
	"sap.ui5": {
		"config": {
			"shortpoint": {}
		},
		"_version": "1.1.0",
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "shortpoint-widget-beta.i18n.i18n"
				}
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.hana.uis.flp.routing.Router"
			}
		},
		"componentName": "shortpoint-widget-beta"
	},
	"sap.flp": {
		"appType": "SAP_UI5_Component",
		"config": {
			"editable": true,
			"deletable": true
		},
		"_version": "1.1.0",
		"type": "application"
	},
	"sap.platform.hcp": {
		"uri": "webapp",
		"appName": "shortpointwidgetbeta",
		"_version": "1.1.0"
	},
	"sap.cloud.portal": {
		"show.portal.settings": false,
		"settings": {
			"markup": {
				"isRequired": false,
				"storage_type": "html",
				"value": "[section][section-item][row][column 12][info color=\"warning\" icon=\"info\"]<div>Press the Edit icon to start designing with ShortPoint, If you want to add a new ShortPoint element, you need to delete this element first.</div>[/info][/column][/row][/section-item][/section]",
				"displayName": "HTML",
				"id": "markup"
			}
		},
		"menu": {
			"actions": {
				"properties": {
					"events": {
						"press": "onEditButtonPress"
					},
					"attributes": {
						"icon": "sap-icon://edit",
						"tooltip": "Opens ShortPoint Editor"
					}
				}
			}
		}
	}
}